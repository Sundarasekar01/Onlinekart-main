  <link href="../../../assets/css/theme-rtl.min.css" type="text/css" rel="stylesheet" id="style-rtl">
  <link href="../../../assets/css/theme.min.css" type="text/css" rel="stylesheet" id="style-default">
  <link href="../../../assets/css/user-rtl.min.css" type="text/css" rel="stylesheet" id="user-style-rtl">
  <link href="../../../assets/css/user.min.css" type="text/css" rel="stylesheet" id="user-style-default">
  <script>
    var phoenixIsRTL = window.config.config.phoenixIsRTL;
    if (phoenixIsRTL) {
      var linkDefault = document.getElementById('style-default');
      var userLinkDefault = document.getElementById('user-style-default');
      linkDefault.setAttribute('disabled', true);
      userLinkDefault.setAttribute('disabled', true);
      document.querySelector('html').setAttribute('dir', 'rtl');
    } else {
      var linkRTL = document.getElementById('style-rtl');
      var userLinkRTL = document.getElementById('user-style-rtl');
      linkRTL.setAttribute('disabled', true);
      userLinkRTL.setAttribute('disabled', true);
    }
  </script>
</head>

<body>
  <!-- ===============================================-->
  <!--    Main Content-->
  <!-- ===============================================-->

 

  <main class="main" id="top">
    <div class="container">
      
      <div class="row flex-center min-vh-100 py-5">
        <div class="col-sm-10 col-md-8 col-lg-5 col-xl-5 col-xxl-3">
          <div class="text-center mb-7">
           
            <div class="bg-light rounded-5 p-4">






                <p class="text-dark">Dear,<span class="fs-5 text-light"></span name="user">
                  <%=user%></span>
                </p>
                <input type="hidden" value="<%=user%>" name="user">
              
              
              
                <% for(let i=0;i<orderDatas.products.item.length;i++){%>
                  <div class="alert  alert-dismissible" style="background-color: rgb(255, 255, 255); width: 25;">




                   

                      <div class="mb-3 text-center" style="margin-left: 18%;">
                        <h5 class="text-dark ">
                          <%=orderDatas.products.item[i].productId.name %>
                        </h5>
                        </div>



                        <div class="mb-3 text-center" style="margin-left: 18%;">

                          <img src="/uploads/<%=orderDatas.products.item[i].productId.image[0]%>" class="img-fluid" alt="lol"
                          width="150px" height="150px" style="border:solid 2px rgb(255, 255, 255); border-radius:20px">

                        </div>



                        
                        <div class="mb-3 text-center" style="margin-left: 18%;">
                        
                          <input class="form-control" placeholder="leave a review here"
                          id="<%=orderDatas.products.item[i].productId._id %>" required></input><br>
                          </div>


                        <div class="row">
<div class="col-4">

  <div class="mb-3 text-center">

    <button type="submit"  onclick="del('<%=orderDatas.products.item[i].productId._id%>',event)" class="btn btn-primary ms-5">Post
    </button>

  </div>

</div>



<div class="col-4">

  <div class="mb-3 text-center">
    <a href="#" class="close" style="margin-left: 100%;" data-dismiss="alert" aria-label="close">
      <button class="btn btn-danger btn-sm">Cancel</button>
    </a>
  </div>
</div>


</div>
              
                    

                        
              
                       
                  </div>
                  <% } %>
                    <a href="/userProfile" class="btn btn-primary">Profile</a>
            
              
              


            </div>
            

          </div>
        </div>
      </div>


  </main>


  
  <script>
    function del(id, event) {
      rev = document.getElementById(id).value
      id = id
      var alertDiv = event.target.closest('.alert');
      alertDiv.parentNode.removeChild(alertDiv);
  
      $.ajax({
        url: `/writeReview`,
        type: 'POST',
        data: { rev, id },
  
      })
  
    }
  
  </script>
  





